{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/test_icon.png"
    },
    "content_scripts": [
      {
        "js": ["scripts/page-manip.js", "scripts/local-storage.js", "scripts/api.js"],
        "matches": [
          "https://developer.chrome.com/docs/extensions/*",
          "https://developer.chrome.com/docs/webstore/*"
        ]
      }
    ],
    "web_accessible_resources": [{
      "matches": ["<all_urls>"],
      "resources": ["scripts/*"]
    }],
    "permissions": [
      "storage"
    ]
  }
  